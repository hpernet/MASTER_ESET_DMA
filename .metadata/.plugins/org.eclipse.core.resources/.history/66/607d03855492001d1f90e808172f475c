// Include
#include <stdint.h>
#include <stm32f446xx.h>
#include <stdio.h>
#include "main.h"
#include "gpio.h"
#include "usart.h"

// Private functions
int __io_putchar(int ch)
{
	while(((USART2->SR)&= (1<<7))==0);
	USART2->DR = ch;
}

// Main
int main(void)
{
	// Initialize peripherals
	GPIO_Init();
	USART2_Init();

	// Infinite loop
	while(1)
	{
		for(uint32_t i = 0 ; i<50000; i++);
		GPIOA->ODR ^= 1<<5;
		printf("This is Application0\r\n");
	}
}

void button_is_pressed(void)
{
	printf("Push button pressed\r\n");

}

// End of file
